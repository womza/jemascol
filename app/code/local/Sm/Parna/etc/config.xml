<?xml version="1.0"?>
<config>
	<modules>
		<Sm_Parna>
			<version>1.0.0</version>
			<title>MagenTech.com</title>
			<link><![CDATA[http://www.magentech.com/]]></link>
		</Sm_Parna>
	</modules>
	<global>
		<helpers>
		  <parna>
			<class>Sm_Parna_Helper</class>
		  </parna>
		</helpers>
		<models>
		  <parna>
			<class>Sm_Parna_Model</class>
		  </parna>
		</models>		
	</global>
	<adminhtml>
        <translate>
            <modules>
                <Sm_Parna>
                    <files>
                        <default>Sm_Parna.csv</default>
                    </files>
                </Sm_Parna>
            </modules>
        </translate>
	</adminhtml>	
 	<default>
		<parna_cfg module="parna">
			<general>
				<showcpanel>1</showcpanel>
				<layout_styles>1</layout_styles>
				<color>pink</color>
				<body_background_color>#ffffff</body_background_color>
				<body_link_color>#666666</body_link_color>
				<body_text_color>#666666</body_text_color>
				<menu_styles>1</menu_styles>
				<google_font>Roboto Condensed</google_font>
				<google_font_targets></google_font_targets>
				<body_font_size>12px</body_font_size>
				<body_font_family>Arial</body_font_family>
				<responsive_menu>collapse</responsive_menu>
			</general>
		</parna_cfg>
	</default> 
	 <frontend>
		<events>
			<catalog_product_upsell>
				<observers>
					<bundle_observer>
						<type>disabled</type>
					</bundle_observer>
				</observers>
			</catalog_product_upsell>
		</events>
	</frontend> 
</config> 